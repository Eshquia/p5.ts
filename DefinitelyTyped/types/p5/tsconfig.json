{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["dom", "es5"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "./src/*.d.ts",
        "./lib/*.d.ts",
        "./index.d.ts",
        "./literals.d.ts",
        "./constants.d.ts",
        "./test/*.ts"
    ]
}
