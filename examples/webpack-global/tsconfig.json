{
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "module": "es6",
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@types",
      "../../DefinitelyTyped/types"
    ],
    "paths": {
      "*": ["../../DefinitelyTyped/types/*"]
    }
  },
  "include": [
    "src/**/*",
    "../../DefinitelyTyped/types/p5/global.d.ts"
  ]
}
